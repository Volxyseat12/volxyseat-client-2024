{
  "version": 2,
  "builds": [
    {
      "src": "src/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/volxyseat-client-2024"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/payments/(.*)",
      "dest": "/payments/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}